				["at0076"] = <
					text = <"5 - Jordbruksbedrift, skogbruk">
					description = <"Gårdstun, driftsbygninger, dyrket mark, gårdsvei, gartneri, område for reindrift. Eksklusiv: Bolig inne (B1). Bolig ute (B2). Hage (B2). Utmark (N).">
				>
				["at0077"] = <
					text = <"6 - Institusjon, sykehus, syke-, aldershjem">
					description = <"Fritidshjem, ungdomsklubb, døgninstitusjoner for barn og unge, sykehus og helseinstitusjon, kaserne, fengsel, offentlig tilgjengelig kontor, foreningslokale, museum, kirke.">
				>
				["at0078"] = <
					text = <"7 - Lekeplass">
					description = <"Opparbeidet lekeplass på skole, SFO, barnehage, park, boligområde, hage (inkl trampoline), byggelekeplass.">
				>
				["at0079"] = <
					text = <"8 - Skole, barnehage, SFO">
					description = <"SFO: skolefritidsordning. Høyskole, universitet, skolegård og familiebarnehage. Eksklusiv: Lekeplass i skole, barnehage, SFO (H). Gymnastikksal, idrettsanlegg, svømmeanlegg på skole, universitet (P).">
				>
				["at0080"] = <
					text = <"9 - Sport og idrettsområde (også på skole, universitet)">
					description = <"Inklusiv: Idrettsanlegg inne og ute med bygninger og tribuner, ballbane, golf- og tennisbane, svømmehall, slalåmbakke og lysløype. Eksklusiv: Preparerte løyper i skog og mark som ikke er lysløyper (N). Svømmebasseng, tennisbane i privat bolig/hage (B1 eller B2).)">
				>
				["at0081"] = <
					text = <"10 - Fri natur, utmark, hav, sjø og vann">
					description = <"Utmark, skog, fjellområde, sti, turvei, preparerte skiløyper, campingplass, militært øvingsområde, brygge, sjøhus, strand, fartøy til vanns og til kai, oljeplattformer. Eksklusiv: Lysløype, bearbeidete idrettsanlegg (P). Dyrket mark (G). Offentlig brygge (A). Hytte (B1 eller B2).">
				>
				["at0082"] = <
					text = <"98 - Annet skadested">
					description = <"Transportområde som stasjon, flyplass, i vogn på skinner (tog, t-bane), parkeringsplass, bensinstasjon. Produksjonsområde som verksted, gruve og anlegg. Butikk, hotell, restaurant, park og fornøyelsesområde.">
